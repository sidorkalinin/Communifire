        <splash height="2732" src="resources/ios/splash/Default@2x~universal~anyany.png" width="2732" />
        <preference name="APP_SECRET" value="49f5c6a1-f72f-4517-9b54-1d7bed7abc61" />
    </platform>
    <feature name="CDVWKWebViewEngine">
        <param name="ios-package" value="CDVWKWebViewEngine" />
    </feature>
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <preference name="SplashScreenDelay" value="3000" />
    <edit-config file="*-Info.plist" mode="merge" target="NSMicrophoneUsageDescription">
        <string>Allow the app to use your recorded videos with sound to share them with your coworkers.</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>Allow the app to access your photos to share them with your coworkers.</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
        <string>Allow the app to use your camera to share your photos with your coworkers.</string>
    </edit-config>
    <allow-navigation href="*" />
    <allow-navigation href="https://*youtube.com/*" />
    <allow-navigation href="https://*vimeo.com/*" />
    <allow-navigation href="https://*docs.google.com/*" />
    <allow-navigation href="http://192.168.1.7:8100" />
    <allow-navigation href="http://192.168.0.5:8100" />
    <allow-navigation href="http://192.168.0.2:8100" />
    <allow-navigation href="http://192.168.1.11:8100" />
    <allow-navigation href="http://192.168.0.4:8100" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="osx" spec="^4.0.2" />
    <engine name="ios" spec="4.5.5" />
    <engine name="android" spec="7.1.1" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-device" spec="^1.1.7" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.1.0" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1" />
    <plugin name="cordova-sqlite-storage" spec="^2.4.0" />
    <plugin name="cordova-plugin-camera" spec="^2.4.1">
        <variable name="MICROPHONE_USAGE_DESCRIPTION" value="Allow the app to use your recorded videos with sound to share them with your coworkers." />
        <variable name="CAMERA_USAGE_DESCRIPTION" value="Allow the app to use your camera to share your photos with your coworkers." />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="Allow the app to access your photos to share them with your coworkers." />
    </plugin>
    <plugin name="cordova-plugin-ionic-webview" spec="^1.2.1" />
    <plugin name="cordova-plugin-vibration" spec="^2.1.6" />
    <plugin name="cordova-plugin-media-capture" spec="^2.0.0">
        <variable name="MICROPHONE_USAGE_DESCRIPTION" value="Allow the app to use your recorded videos with sound to share them with your coworkers." />
        <variable name="CAMERA_USAGE_DESCRIPTION" value="Allow the app to use your camera to share your photos with your coworkers." />
    </plugin>
    <plugin name="cordova-plugin-taptic-engine" spec="^2.1.0" />
    <plugin name="cordova-plugin-fullscreen" spec="^1.2.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^2.0.2" />
    <plugin name="cordova-plugin-filechooser" spec="^1.0.1" />
    <plugin name="cordova-plugin-badge" spec="^0.8.7" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-filepath" spec="^1.4.2" />
    <plugin name="cordova-plugin-ios-non-exempt-encryption" spec="^1.0.0" />
    <plugin name="cordova-plugin-crop" spec="^0.4.0" />
    <plugin name="cordova-plugin-video-editor" spec="^1.1.3" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-file" spec="^5.0.0" />
    <plugin name="cordova-plugin-appcenter-analytics" spec="^0.1.8" />
    <plugin name="cordova-plugin-appcenter-crashes" spec="^0.1.8" />
    <plugin name="cordova-plugin-filepicker" spec="^1.1.5" />
    <plugin name="phonegap-plugin-push" spec="^2.2.3">
        <variable name="SENDER_ID" value="1066246631997" />
    </plugin>
    <plugin name="cordova-plugin-document-viewer" spec="^0.9.10" />
    <plugin name="cordova-plugin-add-swift-support" spec="^1.7.2" />
    <plugin name="cordova-plugin-photo-library" spec="^2.1.1">
        <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="ionic-plugin-deeplinks" spec="^1.0.17">
        <variable name="URL_SCHEME" value="communifire" />
        <variable name="DEEPLINK_SCHEME" value="https" />
        <variable name="DEEPLINK_HOST" value="*.communifire.com" />
        <variable name="ANDROID_PATH_PREFIX" value="/" />
    </plugin>
</widget>
